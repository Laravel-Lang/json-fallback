{
    "name": "laravel-lang/json-fallback-hotfix",
    "description": "Fixing a bug with processing fallback JSON keys",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com",
            "homepage": "https://github.com/andrey-helldar"
        },
        {
            "name": "Felipe Dsdev",
            "homepage": "https://github.com/felipe-dsdev"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/translation": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "phpunit/phpunit": "^8.5 || ^9.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "LaravelLang\\JsonFallbackHotfix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "friendsofphp/php-cs-fixer": true
        }
    }
}
